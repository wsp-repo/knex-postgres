{
  "name": "@wspro/knex",
  "version": "1.0.0",
  "description": "Knex extends",
  "author": "https://github.com/wsp-repo/",
  "license": "UNLICENSED",
  "keywords": [
    "env",
    "config"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsp-repo/knex.git"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.{ts,js}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "lint:warns": "eslint 'src/**/*.ts' --max-warnings 0",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "test": "jest --config ./jest.unit.js"
  },
  "dependencies": {
    "knex": "3.1.0"
  },
  "devDependencies": {
    "@jest-performance-reporter/core": "2.1.3",
    "@types/jest": "29.5.12",
    "@types/knex": "0.15.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@wspro/linter": "1.0.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-typescript-sort": "0.1.11",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.7",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "package.json": "check-exact-package",
    "*.{js,ts}": [
      "eslint --quiet --max-warnings 0",
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "prettier": "@wspro/linter/prettier"
}
