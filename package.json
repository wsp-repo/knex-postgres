{
  "name": "@zalib/knex-postgres",
  "version": "1.0.7",
  "description": "Knex postgres extends",
  "author": "https://github.com/wsp-repo/",
  "license": "UNLICENSED",
  "keywords": [
    "knex",
    "postgres"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsp-repo/knex-postgres.git"
  },
  "homepage": "https://github.com/wsp-repo/knex-postgres#readme",
  "bugs": {
    "url": "https://github.com/wsp-repo/knex-postgres/issues"
  },
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.{ts,js}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "lint:warns": "eslint 'src/**/*.ts' --max-warnings 0",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "test": "jest --config ./jest.unit.js"
  },
  "dependencies": {
    "knex": "3.1.0"
  },
  "devDependencies": {
    "@jest-performance-reporter/core": "2.1.3",
    "@types/jest": "29.5.12",
    "@types/knex": "0.15.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@zalib/linter": "1.0.9",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-typescript-sort": "0.1.11",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.7",
    "prettier": "3.5.3",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "lint-staged": {
    "package.json": "check-exact-package",
    "*.{js,ts}": [
      "eslint --quiet --max-warnings 0",
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "prettier": "@zalib/linter/prettier"
}
